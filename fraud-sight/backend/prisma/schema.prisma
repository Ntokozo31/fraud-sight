
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id         Int       @id @default(autoincrement())
  amount     Float
  type       String
  createdAt  DateTime  @default(now())
}
